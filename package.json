{
  "name": "voting-app",
  "version": "0.0.1",
  "description": "VotingApp",
  "main": "index.js",
  "engines": {
    "node": ">=10.6.0"
  },
  "scripts": {
    "publish": "node release.js",
    "type-check": "tsc -p ./src/client/tsconfig.json",
    "clean": "rimraf dist",
    "postinstall": "npm run build",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "node index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "start:staging": "NODE_ENV=staging node index.js",
    "build-client": "cross-env NODE_ENV=production webpack -p",
    "dev-client": "webpack-dev-server -w",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node index.js\" --noClear",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\""
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gilamran/voting-app"
  },
  "license": "MIT",
  "author": "Gil Amran",
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/find-up": "^2.1.1",
    "chart.js": "^2.8.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.17.3",
    "find-up": "^3.0.0",
    "http-proxy-middleware": "^0.19.1",
    "orbs-client-sdk": "0.0.6",
    "react-chartjs-2": "^2.7.6",
    "request": "^2.88.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "file-loader": "^3.0.1",
    "open-browser-webpack-plugin": "github:rodrigopandini/open-browser-webpack-plugin",
    "postcss-loader": "^3.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tsc-watch": "^1.1.35",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3333",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
